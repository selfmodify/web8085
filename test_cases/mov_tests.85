# testing move instructions

# mov <reg>, a
mvi a,1
mov b,a
mov c,a
mov d,a
mov e,a
mov h,a
mov l,a
.assert a=1 b=1 c=1 d=1 e=1 h=1 l=1

# mov <reg>, b
mvi b,2
mov a,b
mov b,b
mov c,b
mov d,b
mov e,b
mov h,b
mov l,b
.assert a=2 b=2 c=2 b=2 e=2 h=2 l=2

